#!/bin/sh
## icon from wordpress
BASHISH_THEME=chat
_bashish_theme_appchat ()
{
XFONT=ScrB11.pcf.gz
case ${BASHISH_ARGV[0]} in
irssi) EICON=smile.png;TITLE=Irrsi;;
micq) EICON=icq.png;TITLE=mICQ;;
esac
BGCOLOR="#ffffff"
FGCOLOR="#000000"

## these are the ansi colors
## most ones taken from Mandrakes Eterm colors
ANSICOLOR_BLACK="#000000"
ANSICOLOR_RED="#9e1828"
ANSICOLOR_GREEN="#3B7708"
ANSICOLOR_YELLOW="#968a38"
ANSICOLOR_BLUE="#414171"
ANSICOLOR_MAGENTA="#963c59"
ANSICOLOR_CYAN="#84FFFF"
ANSICOLOR_WHITE="#FFFFFF"
ANSICOLOR_BRIGHTBLACK="#D0D0D0"
ANSICOLOR_BRIGHTRED="#cf6171"
ANSICOLOR_BRIGHTGREEN="#76BC0C"
ANSICOLOR_BRIGHTYELLOW="#fff796"
ANSICOLOR_BRIGHTBLUE="#4186be"
ANSICOLOR_BRIGHTMAGENTA="#cf9ebe"
ANSICOLOR_BRIGHTCYAN="#71bebe"
ANSICOLOR_BRIGHTWHITE="#ffffff"
CURSORCOLOR="#9e1828"


}
